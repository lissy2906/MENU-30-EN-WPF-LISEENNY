<UserControl x:Class="EjerciciosWPFLissy.Ejercicio23"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="200" Width="500">
    <StackPanel Margin="20">
        <TextBlock Text="Ejercicio 23: Mayor de dos números" FontSize="16" FontWeight="Bold"/>
        <TextBox x:Name="txtNum1" Margin="0,5"/>
        <TextBox x:Name="txtNum2" Margin="0,5"/>
        <Button Content="Comparar" Click="BtnComparar_Click"/>
        <TextBlock x:Name="txtResultado" Margin="10,10,0,0"/>
    </StackPanel>
</UserControl>

